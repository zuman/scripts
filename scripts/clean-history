#!/usr/bin/python3
import os
from collections import OrderedDict

user = os.path.expanduser('~')
shell = os.environ['SHELL'].split('/')[-1]
file = user+"/."+shell+"_history"
fin = open(file, "r")
uniq = OrderedDict()
for l in fin:
    line = l.strip()
    if line in uniq:
        del(uniq[line])
    uniq[line] = True
fout = open(file, "w")
for line in uniq:
    print(line, file=fout)
fout.close()
print("History cleaned!")
